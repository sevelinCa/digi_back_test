    "@nestjs/platform-express": "10.3.2",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/multer-s3": "3.0.3",
    "@types/prompts": "^2.4.9",
    "apollo-server-express": "^3.13.0",
    "apple-signin-auth": "1.7.5",
    "bcryptjs": "2.4.3",
    "bull": "^4.13.1",
