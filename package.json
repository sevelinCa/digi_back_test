    "@types/jest": "^29.5.12",
    "@types/ms": "0.7.34",
    "@types/multer": "1.4.11",
    "@types/node": "20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-anonymous": "1.0.5",
